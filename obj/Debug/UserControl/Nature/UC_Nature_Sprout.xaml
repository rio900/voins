<UserControl
    x:Class="Voins.UC_Nature_Sprout"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:Voins"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    d:DesignHeight="300"
    d:DesignWidth="400" Width="50" Height="53">
	<UserControl.Resources>
		<Storyboard x:Name="St_Remove"/>
	</UserControl.Resources>
    
    <Grid>
    	<Viewbox x:Name="C_Top" Margin="0" Height="28" Width="16" HorizontalAlignment="Center" VerticalAlignment="Center" Visibility="Collapsed">
    		<Canvas Width="35.7917" Height="55.8719">
                <Image Source="ms-appx:///Image/Nature/Img_Nature_Sprout.png"></Image>
            </Canvas>
    	</Viewbox>
        <Viewbox x:Name="C_Left" Margin="19,16.5" Height="28" Width="16" HorizontalAlignment="Center" VerticalAlignment="Center" RenderTransformOrigin="0.5,0.5" Visibility="Collapsed">
    		<Viewbox.RenderTransform>
    			<CompositeTransform Rotation="-90"/>
    		</Viewbox.RenderTransform>
    		<Canvas Width="35.7917" Height="55.8719">
                <Image Source="ms-appx:///Image/Nature/Img_Nature_Sprout.png"></Image>
    		</Canvas>
    	</Viewbox>
        <Viewbox x:Name="C_Right" Margin="19,16.5" Height="28" Width="16" HorizontalAlignment="Center" VerticalAlignment="Center" RenderTransformOrigin="0.5,0.5" UseLayoutRounding="False" d:LayoutRounding="Auto" Visibility="Collapsed">
    		<Viewbox.RenderTransform>
    			<CompositeTransform Rotation="90"/>
    		</Viewbox.RenderTransform>
    		<Canvas Width="35.7917" Height="55.8719">
                <Image Source="ms-appx:///Image/Nature/Img_Nature_Sprout.png"></Image>
            </Canvas>
    	</Viewbox>
        <Viewbox x:Name="C_Bottom" Margin="19,16.5" Height="28" Width="16" HorizontalAlignment="Center" VerticalAlignment="Center" RenderTransformOrigin="0.5,0.5" UseLayoutRounding="False" d:LayoutRounding="Auto" Visibility="Collapsed">
    		<Viewbox.RenderTransform>
    			<CompositeTransform Rotation="180"/>
    		</Viewbox.RenderTransform>
    		<Canvas Width="35.7917" Height="55.8719">
                <Image Source="ms-appx:///Image/Nature/Img_Nature_Sprout.png"></Image>
            </Canvas>
    	</Viewbox>

    </Grid>
</UserControl>

